<?xml version="1.0"?>
<info>
	<id>data_exporter</id>
	<name>Data Exporter</name>
	<summary>Set of tools to export/import user-data</summary>
	<description>A cli tool which allows exporting and importing user-data like settings, files and shares to migrate data from one instance in to another.</description>
	<licence>GPLv2</licence>
	<author>Ilja Neumann</author>
	<version>0.3.0</version>
	<category>tools</category>
	<namespace>DataExporter</namespace>
	<commands>
		<command>OCA\DataExporter\Command\ImportInstance</command>
		<command>OCA\DataExporter\Command\ExportInstance</command>
		<command>OCA\DataExporter\Command\ExportUser</command>
		<command>OCA\DataExporter\Command\ImportUser</command>
		<command>OCA\DataExporter\Command\MigrateShare</command>
	</commands>
	<dependencies>
		<owncloud min-version="10.12" max-version="10" />
		<nextcloud min-version="9.0.1" max-version="19" />
		<php min-version="7.4" />
	</dependencies>
</info>
